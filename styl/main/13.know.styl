#know
	padding 100px 0
	background-image url(../img/backgrounds/know-bg.png)
	background-repeat no-repeat
	background-position: center
	background-size: cover
.general-title-know
	width 100%
	max-width 750px
.know-title
	font-family: GPm
	font-size: 24px;
	text-shadow: 0px 4px 20px rgba(114, 238, 255, 0.3);
	margin-bottom: 20px
	padding-left: 80px
	background-repeat no-repeat
	background-position: left top
	background-size: 75px
	&-1
		background-image url(../img/icons/know-icon-1.png)
	&-2
		background-image url(../img/icons/know-icon-2.png)
.know-list
	li
		list-style-position: inside
		font-family: GPBl
		color: myG
		text-shadow: 0px 4px 20px rgba(0, 0, 0, 0.25);
		margin-bottom: 20px
		span
			color #D9F6FF;
			font-family: GP
.button-know
	margin 40px 0 0