#targets
	padding 100px 0 50px
.targets-main-title
	font-family: GPBk
	font-size: 120px;
	// line-height: 100px;
	color: rgba(9, 60, 67, 0.5);
	transform: rotate(-90deg);
	position relative
	top 135px
.target
	padding-left: 105px
	background-image url(../img/icons/target.png)
	background-repeat no-repeat
	background-position left top
	&:last-of-type
		margin-top: 40px
	&-title
		font-size: 24px;
		line-height: 42px;
		text-shadow: 0px 4px 20px rgba(114, 238, 255, 0.3);
		margin-bottom: 15px