#get
	padding 50px 0 100px
	background-image url(../img/images/planet-for.png), url(../img/images/planet-for.png),url(../img/images/planet-for.png)
	background-repeat no-repeat, no-repeat, no-repeat
	background-position: left -20px top -30px, left -40px top 330px, left -40px top 250px
	background-size: 405px, 202px, 69px
.get-img
	width 100%
	box-shadow: 20px 20px 60px rgba(114, 238, 255, 0.2);
	border-radius: 5px;
	// margin-right: 50px
	// display block
	&-1
		margin-bottom: 32px
.get-list
	width 100%
	max-width 586px
	li
		padding-left: 70px
		background-repeat no-repeat
		background-position: left top
		background-size: 59px
		margin-bottom: 30px
		min-height 60px
		list-style none
		&:first-child
			background-image url(../img/icons/get-icon-1.png)
		&:nth-child(2)
			background-image url(../img/icons/get-icon-2.png)
		&:nth-child(3)
			background-image url(../img/icons/get-icon-3.png)
		&:nth-child(4)
			background-image url(../img/icons/get-icon-4.png)
		&:last-child
			background-image url(../img/icons/get-icon-5.png)
			margin-bottom: 0