@media screen and (min-width 2560px)
	body
		font-size: 18px
	.container
		width 1400px
	.stat-1
		margin 0 80px 0 250px
	.stat-2
		margin 80px 80px 0 0
	.author-img
		width 100%
	.cert-link
		margin 0 auto 40px
@media screen and (max-width 1199px)
	header
		padding 15px 0
		margin-bottom: 30px
	#logo
		float: none
		margin 0 auto 5px
		display block
	.contacts
		float: none
		text-align: center
	.contact
		display: inline-block
		margin 5px 5px 0
		line-height: 17px
	.main-subtitle
		margin 30px 0 30px 0
	.main-list
		margin-left: 0
	.video-wrapper-main
		margin-top: 40px
	.stat-1
		margin 0 20px 0 25px
	.author-img
		width 100%
	.author-history
		margin-top: 20px
	.fact-title
		line-height: .8
		margin-bottom: 10px
		small
			bottom 11px
	#what
	#about
	#methods
	#targets
	#for
	#get
	#plan
	#earns
	#mail-section
	#authors
	#facts
	#know
	#plan-2
	#materials
	#revs
	#bonuses
		padding 50px 0
	#last
		padding 50px 0 0
	footer 
		margin-top: 100px
@media screen and (max-width 991px)
	.main-list
		max-width 700px
	.main-img
		max-width 380px
		margin-top: 30px
	.video-wrapper-main
		margin-top: 0
	.stat
		float: none
		margin: 0 auto 20px
	.stat-subtitle
	.stat-text
		width 100%
	#what .container
		padding 460px 15px 0
		background-position: center top
	.methods:before
		content: none
	.general-text-methods
		width 100%
		max-width 300px
		&:last-of-type
			margin 0 auto 25px
		&:before
			margin-bottom: 10px
	.targets-main-title
		position: absolute
		right -100px
	.target
		margin-top: 40px
	#for
		background-position: right -120px top 250px, right -200px top
	#get
		background-position: left -254px top -30px, left -140px top 330px, left -40px top 250px
	.get-list
		margin-bottom: 25px
		max-width 100%
	.plan-main-title
		right -307px
	#plan
	#targets
		overflow hidden
	.general-title-mail-section
		text-align: center
	.material-subtitle
		font-size: 22px
	.carousel-control
		top auto
		bottom: -60px
	#text-revs .rev-img
		height 300px
	.bonus
		margin-bottom: 45px
		text-align: center
	#last
		text-align: center
@media screen and (max-width 767px)
	.stat-main-title
		position: static
		transform: none
		font-size: 70px
	.general-title
	.general-text-about, .general-text-what
		text-align: center
		max-width 100%
	.targets-main-title
		display: none
	.for-list
		max-width 550px
		margin-bottom: 45px
	.earn
		width 100%
		max-width 380px
		margin 30px auto 0
		text-align: center
		background-position: top center
		&-1
			padding-top:  148px
	.author-single
		width 100%
		max-width 320px
		margin 0 auto 20px
	.author-title
		font-size: 28px
		left 20px
		bottom: 20px
	.author-certs-title
		text-align: center
	.fact
	.material
		margin-top: 20px
	.know-title
	.plan-2
		max-width 550px
	.plan-2-img
		margin-top: 40px
	.certs
		width 100%
		max-width 350px
		margin 0 auto 25px
	#for
	#get
		background-image: none
